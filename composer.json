{
  "name": "idavoll/kantoku",
  "type": "module",
  "description": "Kantoku module for AsgardCMS. Handles the activation of modules.",
  "keywords": [
    "asgardcms",
    "kantoku",
    "modules"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Nicolas Widart",
    "email": "info@asgardcms.com",
    "role": "Developer"
  }],
  "support": {
    "email": "support@asgardcms.com",
    "issues": "https://github.com/AsgardCms/Platform/issues",
    "source": "https://github.com/AsgardCms/Kantoku"
  },
  "require": {
    "php": ">=7.0.0",
    "composer/installers": "~1.0",
    "symfony/yaml": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "3.5.*",
    "spatie/phpunit-snapshot-assertions": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Kantoku\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}